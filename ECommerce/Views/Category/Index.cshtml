@model List<Category>


<h1>Category List</h1>
<table class="table table-bordered table-striped">
    <thead>
        <th>Category Name</th>
        <th>Display Order</th>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.DisplayOrder</td>
            </tr>
        }
       
    </tbody>

</table>





